{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "discount": {
      "type": "integer",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "rating": {
      "type": "decimal",
      "required": true
    },
    "ratingCount": {
      "type": "integer",
      "required": true
    },
    "comment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::comment.comment",
      "mappedBy": "product"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    }
  }
}
